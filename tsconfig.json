{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["dom", "es2015", "es2016", "es2017", "es2020", "esnext"],
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
